			value="${datasource.url:jdbc:mysql://tds-db.dev.opentestsystem.org:3306?useUnicode=true&amp;useFastDateParsing=false&amp;characterEncoding=UTF-8}" />
		<beans:property name="username" value="${datasource.username:dbsbac}" />
		<beans:property name="password"
			value="${datasource.password:TkLsZdWnH72JDb}" />
	</beans:bean>


	<beans:bean id="_tdsAdminDLL" class="tds.dll.mysql.TDSAdminDLL" />
	<beans:bean id="tdsSettings" class="TDS.Shared.Configuration.TDSSettings"
		scope="prototype" />
